create
	int bot n
		on activation:
			recieve.
		end

		on default:
			store me - 1.
		end
	end

	int bot f
		on activation:
			store 0.
			send.
			drop 0.
			store 1.
		end

		on default:
			send.
			collect as x.
			drop me.
			store me + x.
		end
	end
	
execute
	activate n, f.
	while n <= 0:
		advance n, f.
	end
end