create
    int bot bot4
    	on activation :
    		collect as x.
            store x.
    	end
    end

    char bot bot5
        on default:
            collect as x.
            drop 'h'.
            store x.
        end
    end

execute

    activate bot4,bot5.

    if 1 > 2 : 
        deactivate bot5.
    end
    advance bot4.
end 