$$ ERROR CON EL 'me'
create 

int bot a,b,c,d,f,g 
   on true:
       read as x.
       read as p.
       collect as a.
       collect as b.
       collect as c.
       collect as d.
    end
end

bool bot m,z  
     on activation:
         store me.
     end
end 

execute
	activate d.
   create
      int bot bot1,bot2 
      on activation:
          store me+1.
          read as y.
      end
   end
   execute
       deactivate a.
       if me/\true\/false: $$ No se puede usar variable me en este alcance
           activate bot1.
       end
      create
          int bot ae,be,ce
             on activation:
                 read as x.
                 read as p.
             end
          end
       execute
           activate bot1,a,b,c,d,ae.
           activate be.
           activate bot1.
           
       end
       activate bot1.
   end 
   advance a.
end
